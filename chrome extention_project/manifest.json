{
  "name": "Meet Joinees Notifier",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Adds the ability to receive a notification when a participant joins a Google Meet",
  "author": "<YOUR_EMAIL>",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "notifications"
  ]
}
